{
  "expo": {
    "name": "RNGains",
    "slug": "RNGains",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "rngains",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.TheKian.RNGains",
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
      },
      "package": "com.TheKian.RNGains",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
    },
    "plugins": [
      "@react-native-firebase/app",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "02640ed0-b0ba-4f9e-b438-74df458b6d98"
      }
    }
  }
}
